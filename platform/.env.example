# API Configuration
VITE_API_URL=https://your-api-gateway-url.execute-api.us-west-2.amazonaws.com/prod
VITE_API_KEY=your_api_gateway_key

# AWS Configuration
VITE_AWS_REGION=us-west-2
VITE_AWS_ACCESS_KEY_ID=your_access_key_id
VITE_AWS_SECRET_ACCESS_KEY=your_secret_access_key

# AWS Cognito (Authentication)
VITE_AWS_COGNITO_USER_POOL_ID=us-west-2_xxxxxxxxx
VITE_AWS_COGNITO_CLIENT_ID=your_cognito_client_id
VITE_AWS_COGNITO_DOMAIN=your-cognito-domain.auth.us-west-2.amazoncognito.com

# AWS Bedrock (AI Agents)
VITE_AWS_BEDROCK_AGENT_ID=your_bedrock_agent_id
VITE_AWS_BEDROCK_AGENT_ALIAS_ID=your_bedrock_agent_alias_id
VITE_AWS_BEDROCK_MODEL_ID=anthropic.claude-3-5-sonnet-20241022-v2:0

# AWS SageMaker (Optimization Jobs)
VITE_AWS_SAGEMAKER_ENDPOINT_NAME=numerix-optimization-endpoint
VITE_AWS_SAGEMAKER_ROLE_ARN=arn:aws:iam::your-account-id:role/SageMakerExecutionRole

# AWS S3 (Results Storage)
VITE_AWS_S3_RESULTS_BUCKET=numerix-optimization-results
VITE_AWS_S3_DATA_BUCKET=numerix-market-data

# Feature Flags
VITE_ENABLE_MOCK_DATA=true
VITE_ENABLE_DEBUG=false
VITE_ENABLE_WEBSOCKET=false
VITE_ENABLE_EXPORT=false
